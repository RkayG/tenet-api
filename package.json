{
  "name": "@tenet/api",
  "version": "1.0.0",
  "description": "Enterprise-grade API framework with built-in security, audit trails, multi-tenancy, and compliance features for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "lint:security": "eslint src/**/*.ts --plugin security --plugin security-node",
    "lint:report": "eslint src/**/*.ts --format json --output-file eslint-report.json",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "docker:build": "docker build -t tenet-api .",
    "docker:run": "docker run -p 3000:3000 tenet-api",
    "docs": "typedoc src/index.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "audit:production": "npm audit --production",
    "audit:json": "npm audit --json",
    "security:check": "npm audit --audit-level=moderate",
    "analyze": "npm run lint && npm run type-check && npm run audit",
    "analyze:sonar": "sonar-scanner"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RkayG/tenet.git"
  },
  "keywords": [
    "tenet",
    "api-framework",
    "security",
    "multi-tenant",
    "authentication",
    "audit-trail",
    "compliance",
    "gdpr",
    "soc2",
    "hipaa",
    "encryption",
    "rate-limiting",
    "prisma",
    "typescript",
    "enterprise"
  ],
  "author": "RkayG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RkayG/tenet/issues"
  },
  "homepage": "https://tenetapi.com",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dompurify": "^3.0.6",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.9.2",
    "jsdom": "^27.4.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.6.0",
    "redis": "^4.6.8",
    "sanitize-html": "^2.11.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^29.5.5",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.8.7",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-security-node": "^1.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.21.0",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "mysql2": "^3.6.5",
    "pg": "^8.11.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}